<?xml version="1.0"?>
<openerp>
    <data>
    <!-- Menus -->
        <menuitem id="base.menu_partner_address_form" name="Addresses" parent="account.menu_finance_receivables" sequence="50"/>
        <menuitem id="account.menu_finance_receivables" name="Donors" sequence="50" web_icon="images/donation_box_icon_bw.jpg"
            web_icon_hover="images/donation_box_icon_hover.jpg" action="base.action_partner_customer_form"/>
        <menuitem id="account.menu_finance_payables" name="People and Projects" sequence="60" web_icon="images/people_and_projects.png"
            web_icon_hover="images/people_and_projects_hover.png"/>
        <menuitem id="base.menu_base_partner" groups="base.group_no_one"/>
        <menuitem id="account.menu_action_invoice_tree1" groups="base.group_no_one"/>
        <menuitem id="account.menu_action_invoice_tree3" groups="base.group_no_one"/>
        <menuitem id="account_voucher.menu_action_sale_receipt" groups="base.group_no_one"/>
        <menuitem id="account.menu_account_customer" name="Donors Information" parent="account.menu_finance_receivables"/>
        <menuitem id="account_voucher.menu_action_vendor_receipt" groups="base.group_no_one"/>
        <menuitem id="fund_transfers" web_icon="images/fund_transfer.png" name="Fund Transfers"
            web_icon_hover="" sequence="80"/>
        <menuitem id="fund_transfer_b2b" parent="fund_transfers" sequence="10" action="action_fund_transfer_b2b_tree"/>
        <menuitem id="fund_transfer_a2a" parent="fund_transfers" sequence="20" action="action_fund_transfer_a2a_tree"/>
      <!-- Actions -->
        <record id="base.action_partner_customer_form" model="ir.actions.act_window">
            <field name="name">Donors Information</field>
        </record>
        
      <!-- Views Override -->
        <record id="base.view_partner_tree" model="ir.ui.view">
            <field name="name">res.partner.tree</field>
            <field name="model">res.partner</field>
            <field name="type">tree</field>
            <field eval="8" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Entity">
                    <field name="name"/>
                    <field name="ref" groups="base.group_extended"/>
                    <field name="phone"/>
                    <field name="email"/>
                    <field name="city"/>
                    <field name="country"/>
                    <field name="user_id"/>
                </tree>
            </field>
        </record>
       </data>
</openerp>
